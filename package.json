{
  "name": "mla-projects",
  "version": "1.0.0",
  "private": true,
  "node": "14.15.0",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --fix \"**/*.{vue,ts}\" --ignore-path .gitignore",
    "lint:style": "stylelint --fix \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
